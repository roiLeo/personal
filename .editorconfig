# editorconfig.org
root = true

[*]
indent_size = 2
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

# Quote override for all JS
[*.js]
quote_type = single

# Quote override for all VUE
[*.vue]
quote_type = single

# Quote override for all TS
[*.ts]
quote_type = single
